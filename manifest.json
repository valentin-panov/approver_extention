{
  "manifest_version": 2,
  "name": "The FPR approve extension",
  "version": "1.0.0",
  "description": "Cleans merge request titles for SSC use",
  "icons": {
    "48": "icons/approve_48.png",
    "96": "icons/approve_96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://172.26.149.20/ssc/html/ssc/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "approver_ext@luxoft.com"
    }
  }
}